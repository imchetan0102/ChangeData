<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="service1"
     targetNamespace="http://xmlns.oracle.com/POprocessing/CountNum/service1"
     xmlns:tns="http://xmlns.oracle.com/POprocessing/CountNum/service1"
     xmlns:inp1="http://www.example.org"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://www.example.org" schemaLocation="../Schemas/count.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="input" element="inp1:Input"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="output" element="inp1:Output"/>
    </wsdl:message>
    <wsdl:portType name="execute_ptt">
        <wsdl:operation name="Addition">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
        </wsdl:operation>
         <wsdl:operation name="Substraction">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
